var a = 0;
var b = 3;
var continue = true;

while (a < 10 and continue) {
    print "Hello, World!";
    a = a + 1;

    if (a >= b) {
        continue = false;
    }
}

for (var i = 0; i < 4; i = i + 1) {
    print "Hello, For Loop!";
}

